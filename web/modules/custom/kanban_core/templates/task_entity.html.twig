{#
/**
 * @file task_entity.html.twig
 * Default theme implementation to present Task entity data.
 *
 * This template is used when viewing Task entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_task_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('task_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
